
@{
    ViewBag.Title = "Products";
    Layout = "~/Views/Layout.cshtml";
}
@model IEnumerable<Wriststone.Models.Table.Product>
<div class="profile_menu">
    <ul>
        <li><a href="/Profile/Profile/@Session["id"]">Profile</a></li>
        <li class="active"><a>Products</a></li>
        <li><button type="button" onclick="window.location='/Profile/Edit'">Edit profile</button></li>
    </ul>
</div>

<div class="list">
    <h2>Bought products</h2>
    <ul>
        @foreach (var item in Model) { <li><a href="/Store/Product/@item.Id">@item.Name</a></li> }
    </ul>
</div>

